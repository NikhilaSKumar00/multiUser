@model multiUser.Models.Logincls

    @{
        ViewBag.Title = "Login_pageload";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>Login_pageload</h2>



    <div>
        @using (Html.BeginForm("Login_click", "UserLogin", FormMethod.Get))

        {
            


            <div><label>Username:</label></div>
            <div>
                @Html.TextBoxFor(m => m.Uname)
                @Html.ValidationMessageFor(m => m.Uname)
            </div>

            <div><label>Password:</label></div>
            <div>
                @Html.PasswordFor(m => m.password)
                @Html.ValidationMessageFor(m => m.password)
            </div>


            <div>
                <input type="submit" value="insert" />
                @Html.DisplayFor(m => m.msg)
            </div>
        }
    </div>